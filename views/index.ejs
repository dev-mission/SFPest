<div class="home-pano jumbotron jumbotron-fluid">
  <div class="home-pano__caption text-right">
    <a href="https://flic.kr/p/7ckvWx" target="_blank">moonrise from San Francisco by Art Siegel</a>
    <a href="https://creativecommons.org/licenses/by-nc/2.0/" target="_blank"><i class="fab fa-creative-commons"></i> <i class="fab fa-creative-commons-by"></i> <i class="fab fa-creative-commons-nc"></i> (CC BY-NC 2.0)</a>
  </div>
</div>
<div class="home-properties container">
  <div class="row justify-content-center">
    <div class="col-sm-8 col-md-6 col-lg-4">
      <div class="card mt-5">
        <div class="card-header">
          <h1 class="card-title h5 mb-0">Report pest problems at:</h1>
        </div>
        <ul class="list-group list-group-flush">
          <% for (let property of properties) { %>
          <li class="list-group-item">
            <a href="/<%= property.urlId %>"><%= property.name %> <i class="fas fa-arrow-circle-right"></i></a>
          </li>
          <% } %>
        </ul>
      </div>
    </div>
  </div>
</div>
<div class="container">
  <div class="row justify-content-center">
    <div class="col-sm-6 col-md-4 col-lg-3">
      <a class="card mt-3" href="/about-us">
        <div class="card-body text-center">
          <div class="home-circle mb-2">
            <i class="fas fa-question"></i>
          </div>
          About SF Pest
        </div>
      </a>
    </div>
    <div class="col-sm-6 col-md-4 col-lg-3">
      <a class="card mt-3" href="https://www.pestec.com/pest-library/">
        <div class="card-body text-center">
          <div class="home-circle mb-2">
            <i class="fas fa-bug"></i>
          </div>
          <img src="/images/pestec.jpg" alt="Pestec" style="width: auto; height: 20px;"> Pest Library<br>
        </div>
      </a>
    </div>
  </div>
</div>
